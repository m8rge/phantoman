{
    "name": "site5/phantoman",
    "description": "The Codeception extension for automatically starting and stopping Chromedriver when running tests.",
    "keywords": ["codeception", "extension", "phantomjs", "chrome", "chromedriver", "headless"],
    "homepage": "https://github.com/m8rge/phantoman",
    "license": "MIT",
    "authors": [
        {
            "name": "Grant Lucas",
            "email": "contact@grantlucas.com"
        }
    ],
    "require": {
        "php": ">=5.4.0",
        "codeception/codeception": "^2.2.7"
    },
    "autoload": {
        "psr-4": {
          "Codeception\\Extension\\": "src/"
        }
    },
    "suggest": {
        "enm1989/chromedriver": "Automatically installs Chromedriver locally to your project."
    }
}
